#Wrapper {
  background: url(<%= asset_path('admin/bg.png') %>) repeat;
}

#Top {
  background: #303033;
}

table.admin {
  td { text-align: center; }
}

#AdminMenu{
  float: left;
  width: 125px;
  margin-left: 50px;
  padding: 10px 0px 10px 10px;
}

#AdminContent {
  margin: 0px 0px 0px 195px;
  padding: 10px;
  .column .box {
    width: 95%;
  }
}

.current_item {
  font-weight: bold;
  color: black !important;
  font-size: 14px;
}

.published {
  color: green;
  font-weight: bold;
}

.draft {
  color: #999;
  font-style: italic;
}

.mll { height: 40em; }

.red { color: red; }
.plane {
  float: left;
  width: 315px;
  margin-right: 20px;
  margin-top: 10px;
}
